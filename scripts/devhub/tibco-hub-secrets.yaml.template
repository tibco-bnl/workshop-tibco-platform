apiVersion: v1
kind: Secret
metadata:
  name: tibco-dp5-devhub-secrets
type: Opaque
data:
  # Replace with your actual GitHub token encoded in base64
  # To use this template:
  # 1. Copy this file to tibco-hub-secrets.yaml
  # 2. Encode your GitHub token: echo -n "your_github_token" | base64
  # 3. Replace YOUR_BASE64_ENCODED_GITHUB_TOKEN_HERE with the encoded value
  # 4. Apply with: kubectl apply -f tibco-hub-secrets.yaml
  GITHUB_TOKEN: YOUR_BASE64_ENCODED_GITHUB_TOKEN_HERE